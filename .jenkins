pipeline {
    agent any
    stages{
        stage ('Build'){
            steps {
                echo "Cleaning the Build"
                mvn clean
            }
        }
        stage ('Deploy'){
            steps {
                echo "Deploying the Build"
            }
        }
        stage ('Test'){
            steps {
                echo "Running Tests on the Build"
                sh "mvn test"
            }
        }
        stage ('Release'){
            steps {
                echo "Releasing the Build"
            }
        }
    }
}